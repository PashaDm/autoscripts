{
  "title": "Game NGM Mobile",
  "customFields": [
    {
      "component": "input",
      "type": "text",
      "name": "gamesPage",
      "default": "/casino",
      "label": "Games page",
      "description": "Usually it is home page",
      "validation": {
        "min": 1,
        "max": 100
      }
    },

    {
      "component": "radio",
      "name": "gameToLaunch",
      "label": "Game to launch",
      "group": [
        { "label": "First", "checked": true  },
        { "label": "Random"  },
        { "label": "By game code:"  }
      ]
    },

    {
      "component": "input",
      "type": "text",
      "name": "gameCode",
      "default": "fnf50",
      "validation": {
        "min": 0,
        "max": 6
      }
    },

    {
      "component": "radio",
      "name": "mode",
      "label": "Game launch mode",
      "group": [
        { "label": "Demo", "checked": true  },
        { "label": "Real"  }
      ],
      "description": "You should be logged in to play for real money. No login action in test."
    },

    {
      "component": "checkbox",
      "name": "preventSwipeUp",
      "label": "Disable swipe up overlay",
      "checked": false
    }
  ]
}